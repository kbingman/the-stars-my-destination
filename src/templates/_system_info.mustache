<div class="system">
  {{# system }}
    <div class="_coords">{{# round }}{{ x }}{{/ round }}:{{# round }}{{ y }}{{/ round }}</div>
    <div class="_star">
      Mass: {{# round }}{{ mass }}{{/ round }}
    </div>
    {{# stars }}
      <div class="_star">
        {{ type }}{{# round }}{{ luminosity }}{{/ round }} &ndash; {{ name }}
      </div>
    {{/ stars }}
    {{# planets }}
      <div class="_planet">
        {{# round }}{{ mass }}{{/ round }}&mdash;{{ gasGiant }}
      </div>
    {{/ planets }}
  {{/ system }}
</div>
